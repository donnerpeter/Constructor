package cons3

/**
 * @author peter
 */
class RelativeClause extends Interceptor implements RussianConstructions {

  @Override
  Collection<Mite> enrichUpdate(Mite self, List<Mite> constructions, ParsingState state) {
    if (constructions.find { it.cxt == relativeClause && it.contents.comma1 }) {
      return [self.unify(relativeClause(comma2:true))]
    }
    return []
  }

}
